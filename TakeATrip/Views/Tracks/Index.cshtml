@model TakeATrip.Models.TracksViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Content/font-awesome.css")

<script src="~/Scripts/tracks.js"></script>

<div>
    <div class="page-header">
        <div class="hidden" id="placesArr">
            @foreach (var placeid in Model.placesId)
            {
                @Html.Raw(placeid + ",")
            }
        </div>
    </div>
   
    <div id="map"></div>

   
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBN-U0Blvdbgj7PUv2_IOXL8QLqTdXRcS0&libraries=places&callback=initMap"
            async defer></script>
</div>
